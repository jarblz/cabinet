<div class="close-x" data-dismiss="modal" aria-label="Close">&Cross;</div>
<div class="row show-row">
  <div class="col-sm-12">
    <h2><%= company.name %></h2>
  </div>
</div>
<div class="row show-row">
  <div class="col-sm-6">
    <div class="show-photo">
      <%= image_tag company.logo.url(:medium) %>
    </div>
  </div>
  <div class="col-sm-6">
    <div class="row">
      <div class="col-sm-12">
        <div class='section-title'>Company Culture</div>
        <p>
          <% company.traits.each do |t| %>
            <span> <%=  t.name %></span>
          <% end %>
        </p>
      </div>
    </div>
  </div>
</div>
